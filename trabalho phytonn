def soma (x, y):
    return x + y 

print(soma(3, 100))
print(soma(3, 7343284328748234))


def simples (cor):
    if cor == 'vermelho':
        return 'escolha certa'
    
def completo (cor):
    if cor == 'vermelho':
        return 'vc e foda'
    elif cor == 'azul':
        return 'vc e top'
    else:
        return 'vsf ein'
    
def medio (cor):
    if cor == 'verde':
      return 'escolha certa'
    else: 
      return 'tente dnv'
